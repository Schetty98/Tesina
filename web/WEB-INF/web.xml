<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>AccediServet</servlet-name>
        <servlet-class>it.schetty.tesina.servlet.AccediServet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccediServet</servlet-name>
        <url-pattern>/AccediServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetMaterieServlet</servlet-name>
        <servlet-class>it.schetty.tesina.servlet.GetMaterieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetMaterieServlet</servlet-name>
        <url-pattern>/GetMaterieServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SaveTextServlet</servlet-name>
        <servlet-class>it.schetty.tesina.servlet.SaveTextServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveTextServlet</servlet-name>
        <url-pattern>/SaveTextServlet</url-pattern>
    </servlet-mapping>
    
</web-app>